<!-- **********************************************************************
        FLIGHT CONTROLS
     *******************************************************************-->
   <flight_control name="FCS: C-5M">

      <channel name="Pitch">

         <summer name="fcs/pitch-control-sum">
            <input> fcs/elevator-cmd-norm </input>
            <input> fcs/pitch-trim-cmd-norm </input>
            <clipto>
               <min> -1.0 </min>
               <max>  1.0 </max>
            </clipto>
            <output> fcs/elevator-pos-norm </output>
         </summer>

         <aerosurface_scale name="fcs/delta-elev-deg">
            <input> fcs/elevator-pos-norm </input>
            <zero_centered> true </zero_centered>
            <domain>
               <min> -1.0 </min>
               <max>  1.0 </max>
            </domain>
            <range>
               <min>-20.00</min>
               <max> 16.00</max>
            </range>
            <output> fcs/elevator-pos-deg </output>
         </aerosurface_scale>

      </channel>


      <channel name="Roll">

         <summer name="fcs/roll-control-sum">
            <input> fcs/aileron-cmd-norm </input>
            <input> fcs/roll-trim-cmd-norm </input>
            <clipto>
               <min> -1.0 </min>
               <max>  1.0 </max>
            </clipto>
            <output> fcs/aileron-pos-norm </output>
         </summer>

         <aerosurface_scale name="fcs/left-aileron">
            <input> fcs/aileron-pos-norm </input>
            <zero_centered> false </zero_centered>
            <domain>
               <min> -1.0 </min>
               <max>  1.0 </max>
            </domain>
            <range>
               <min> -32.00</min>
               <max>  32.00</max>
            </range>
            <output> fcs/left-aileron-pos-deg </output>
         </aerosurface_scale>

         <aerosurface_scale name="fcs/right-aileron">
            <input> -fcs/aileron-pos-norm </input>
            <zero_centered> false </zero_centered>
            <domain>
               <min> -1.0 </min>
               <max>  1.0 </max>
            </domain>
            <range>
               <min>-32.00</min>
               <max> 32.00</max>
            </range>
            <output> fcs/right-aileron-pos-deg </output>
         </aerosurface_scale>

         <summer name="fcs/total-aileron-pos-deg">
            <description> Total control deflection (Left-Right)</description>
            <input>  fcs/left-aileron-pos-deg </input>
            <input> -fcs/right-aileron-pos-deg </input>
         </summer>

      </channel>


      <channel name="Yaw">

         <!-- Datcom doesn't calculate rudder parameters, but this is included for completeness -->
         <summer name="fcs/yaw-control-sum">
            <input> fcs/rudder-cmd-norm </input>
            <input> fcs/yaw-trim-cmd-norm </input>
            <clipto>
               <min> -1.0 </min>
               <max>  1.0 </max>
            </clipto>
            <output>fcs/rudder-pos-norm</output>
         </summer>

         <aerosurface_scale name="fcs/rudder">
            <input> fcs/rudder-pos-norm </input>
            <zero_centered> true </zero_centered>
            <domain>
               <min> -1.0 </min>
               <max>  1.0 </max>
            </domain>
            <range>
               <min> -20.0 </min>
               <max>  20.0 </max>
            </range>
            <output> fcs/rudder-pos-deg </output>
         </aerosurface_scale>

      </channel>


      <channel name="Flaps">

         <kinematic name="flap-control">
            <input> fcs/flap-cmd-norm </input>
            <traverse>
               <setting>
                  <position>  .000 </position>          <!--    .00 deg -->
                  <time>       .00 </time>
               </setting>
               <setting>
                  <position>  .375 </position>          <!--  15.00 deg -->
                  <time>      2.00 </time>
               </setting>
               <setting>
                  <position>  .750 </position>          <!--  30.00 deg -->
                  <time>      4.00 </time>
               </setting>
               <setting>
                  <position> 1.000 </position>          <!--  40.00 deg -->
                  <time>      6.00 </time>
               </setting>
            </traverse>
            <output> fcs/flap-pos-norm </output>
         </kinematic>

         <aerosurface_scale name="fcs/flap-position">
            <input> fcs/flap-pos-norm </input>
               <zero_centered> false </zero_centered>
               <domain>
                  <min>  0.0 </min>
                  <max>  1.0 </max>
               </domain>
               <range>
                  <min>   .00 </min>
                  <max> 40.00 </max>
               </range>
               <output> fcs/flap-pos-deg </output>
         </aerosurface_scale>

      </channel>


      <channel name="Landing Gear">
         <kinematic name="gear-control">
            <input>gear/gear-cmd-norm</input>
            <traverse>
               <setting>
                  <position> 0 </position>
                  <time>     0 </time>
               </setting>
               <setting>
                  <position> 1 </position>
                  <time>     5 </time>
               </setting>
            </traverse>
            <output>gear/gear-pos-norm</output>
         </kinematic>
      </channel>

   </flight_control>


